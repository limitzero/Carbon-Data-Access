<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Carbon.DataAccess.Tests" namespace="Carbon.DataAccess.Tests.Domain" default-lazy="false">
  <!-- entity: OrderLine -->
  <class name="OrderLine" table="OrderLines">
    <id name="Id" column="orderlineID" type="Int32" access="nosetter.camelcase-underscore">
      <generator class="identity" />
    </id>
    <!-- properties for entity 'OrderLine' -->
    <property name="Quantity" column="Quantity" type="Int32" length="200" access="nosetter.lowercase-underscore" />
    <!-- many "OrderLine" are associated with one instance of a "Product" -->
    <many-to-one name="Product" class="Product" cascade="all" access="nosetter.lowercase-underscore" column="productID" foreign-key="fk_Product_has_instances_of_OrderLine" fetch="join" not-found="ignore" />
  </class>
</hibernate-mapping>